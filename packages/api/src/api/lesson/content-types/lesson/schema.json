      "type": "relation",
      "relation": "oneToMany",
      "target": "api::question.question",
      "mappedBy": "lesson"
    },
    "summary": {
      "type": "customField",
      "options": {
        "output": "Markdown",
        "preset": "light"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "artefacts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::artefact.artefact",
      "mappedBy": "lesson"
    }
